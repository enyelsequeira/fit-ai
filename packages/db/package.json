{
  "name": "@fit-ai/db",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./*": {
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:seed:fresh": "tsx src/scripts/seed.ts --fresh",
    "db:seed:exercises": "tsx src/scripts/seed-exercises-d1.ts",
    "db:seed:exercises:fresh": "tsx src/scripts/seed-exercises-d1.ts --fresh",
    "db:seed:exercises:remote": "tsx src/scripts/seed-exercises-d1.ts --remote",
    "db:seed:free-exercise-db": "tsx src/scripts/seed-from-free-exercise-db.ts",
    "db:seed:free-exercise-db:remote": "tsx src/scripts/seed-from-free-exercise-db.ts --remote"
  },
  "dependencies": {
    "@fit-ai/env": "workspace:*",
    "@libsql/client": "catalog:",
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "libsql": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@fit-ai/config": "workspace:*",
    "drizzle-kit": "^0.31.8",
    "drizzle-zod": "^0.8.3",
    "tsx": "^4.19.4",
    "typescript": "catalog:"
  }
}
